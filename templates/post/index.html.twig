{% extends 'base.html.twig' %}
        {% block stylesheets %}
            <link rel="stylesheet" href="style/postsPage.css">
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

{% block title %}Hello PostController!{% endblock %}

{% block body %}
    <nav class="navbar">
        <div class="nav-wrapper">
            <img src="https://logos-download.com/wp-content/uploads/2016/03/Instagram_Logo_2016.png" class="brand-img" alt="">
            <input type="text" class="search-box" placeholder="search">
            <div class="nav-items">
                <img src="data:image/png;base64," class="icon" alt="">
                <img src="data:image/png;base64," class="icon" alt="">
                <img src="data:image/png;base64," class="icon" alt="">
                <img src="data:image/png;base64," class="icon" alt="">
                <img src="data:image/png;base64," class="icon" alt="">
                <div class="icon user-profile">
                    <a href="{{ path('app_user') }}">
                    {% if userConnected.profil.image is not null  %}
                        <img src="{{ vich_uploader_asset(userConnected.profil.image, 'imageFile') }}" class="brand-img"></a>
                    {% else %}
                        <img src="https://media.geeksforgeeks.org/wp-content/uploads/20220609093221/g2-200x200.jpg" class="brand-img" alt="img12">
                    {% endif %}

                </div>
            </div>
        </div>
    </nav>
    <!-- Code for Showing the Status -->
    <main>
        <div class="container">
            <div class="col-9">
                <div class="statuses">
                    {% for relationList in relationList1 %}
                    <div class="status">
                        <div class="image">
                            {% if relationList.freind.profil.image is not null  %}
                                <img src="{{ vich_uploader_asset(relationList.freind.profil.image, 'imageFile') }}" title="{{ relationList.freind.username }}" style="    overflow:hidden;
    -webkit-border-radius:100%;
    -moz-border-radius:100%;
    border-radius:100%;
    width:60px;
    height:60px;" >
                            {% else %}
                                <img src="https://media.geeksforgeeks.org/wp-content/uploads/20220609093221/g2-200x200.jpg" alt="img12">
                            {% endif %}
                        </div>
                    </div>
                    {% endfor %}
                    {% for relationList in relationList2 %}
                        <div class="status">
                            <div class="image">
                                {% if relationList.user.profil.image is not null  %}
                                    <img src="{{ vich_uploader_asset(relationList.user.profil.image, 'imageFile') }}" title="{{ relationList.user.username }}"  style="    overflow:hidden;
    -webkit-border-radius:100%;
    -moz-border-radius:100%;
    border-radius:100%;
    width:60px;
    height:60px;" >
                                {% else %}
                                    <img src="https://media.geeksforgeeks.org/wp-content/uploads/20220609093221/g2-200x200.jpg" alt="img12">
                                {% endif %}
                            </div>
                        </div>
                    {% endfor %}
                </div>

                <!-- Code for viewing the Post -->
                {% for post in posts %}
                <div class="card">
                    <div class="top">
                        <div class="userDetails">
                            <div class="profilepic">
                                <div class="profile_img">
                                    <div class="image">
                                        {% if post.author.profil.image is not null  %}
                                            <img src="{{ vich_uploader_asset(post.author.profil.image, 'imageFile') }}" style="    overflow:hidden;
    -webkit-border-radius:100%;
    -moz-border-radius:100%;
    border-radius:100%;
    width:25px;
    height:25px;" >
                                        {% else %}
                                            <img src="https://media.geeksforgeeks.org/wp-content/uploads/20220609093221/g2-200x200.jpg" alt="img12">
                                        {% endif %}
                                    </div>
                                </div>
                            </div>
                            <h3>
                                {{ post.author.username }}
                            </h3>
                        </div>
                        <div>
							<span class="dot">
								<i class="fas fa-ellipsis-h"></i>
							</span>
                        </div>
                    </div>
                    <div class="imgBx">
                        <img src="{{ vich_uploader_asset(post.image, 'imageFile') }}" class="cover">
                    </div>
                    <div class="bottom">
                        <div class="actionBtns">
                            <div class="left">
								<span class="heart"
                                      onclick="addlike()">
									<span>
										<svg aria-label="Like"
                                             color="#262626"
                                             fill="#262626"
                                             height="24"
                                             role="img"
                                             viewBox="0 0 48 48"
                                             width="24">
											<!-- Coordinate path -->

											<path
                                                    d="M34.6 6.1c5.7 0 10.4 5.2 10.4
												11.5 0 6.8-5.9 11-11.5 16S25 41.3 24
												41.9c-1.1-.7-4.7-4-9.5-8.3-5.7-5-11.5-9.2-11.5-16C3
												11.3 7.7 6.1 13.4 6.1c4.2 0 6.5 2 8.1 4.3
												1.9 2.6 2.2 3.9 2.5 3.9.3 0 .6-1.3 2.5-3.9
												1.6-2.3 3.9-4.3 8.1-4.3m0-3c-4.5 0-7.9
												1.8-10.6 5.6-2.7-3.7-6.1-5.5-10.6-5.5C6 3.1
												0 9.6 0 17.6c0 7.3 5.4 12 10.6 16.5.6.5 1.3
												1.1 1.9 1.7l2.3 2c4.4 3.9 6.6 5.9 7.6 6.5.5.3
												1.1.5 1.6.5.6 0 1.1-.2 1.6-.5 1-.6 2.8-2.2
												7.8-6.8l2-1.8c.7-.6 1.3-1.2 2-1.7C42.7 29.6
												48 25 48 17.6c0-8-6-14.5-13.4-14.5z">
											</path>
										</svg>
									</span>
								</span>
                                <a href="{{ path('show_post',{id:post.id}) }}">
                                    <svg aria-label="Comment"
                                         class="_8-yf5 "
                                         color="#262626"
                                         fill="#262626"
                                         height="24"
                                         role="img"
                                         viewBox="0 0 48 48"
                                         width="24">

                                        <!-- Coordinate path -->
                                        <path clip-rule="evenodd"
                                              d="M47.5 46.1l-2.8-11c1.8-3.3 2.8-7.1 2.8-11.1C47.5
										11 37 .5 24 .5S.5 11 .5 24 11 47.5 24 47.5c4 0
										7.8-1 11.1-2.8l11 2.8c.8.2 1.6-.6 1.4-1.4zm-3-22.1c0
										4-1 7-2.6 10-.2.4-.3.9-.2 1.4l2.1
										8.4-8.3-2.1c-.5-.1-1-.1-1.4.2-1.8 1-5.2 2.6-10
										2.6-11.4 0-20.6-9.2-20.6-20.5S12.7 3.5 24 3.5
										44.5 12.7 44.5 24z"
                                              fill-rule="evenodd">
                                        </path>
                                    </svg>
                                </a>
                                <svg aria-label="Share Post"
                                     class="_8-yf5 "
                                     color="#262626"
                                     fill="#262626"
                                     height="24"
                                     role="img"
                                     viewBox="0 0 48 48"
                                     width="24">
                                    <path
                                            d="M47.8 3.8c-.3-.5-.8-.8-1.3-.8h-45C.9 3.1.3
										3.5.1 4S0 5.2.4 5.7l15.9 15.6 5.5 22.6c.1.6.6
										1 1.2 1.1h.2c.5 0 1-.3
										1.3-.7l23.2-39c.4-.4.4-1 .1-1.5zM5.2
										6.1h35.5L18 18.7 5.2 6.1zm18.7
										33.6l-4.4-18.4L42.4 8.6 23.9 39.7z">
                                    </path>
                                </svg>
                            </div>
                            <div class="right">
                                <svg aria-label="Save"
                                     class="_8-yf5 "
                                     color="#262626"
                                     fill="#262626"
                                     height="24"
                                     role="img"
                                     viewBox="0 0 48 48"
                                     width="24">

                                    <!-- Coordinate path -->
                                    <path
                                            d="M43.5 48c-.4 0-.8-.2-1.1-.4L24 29 5.6
										47.6c-.4.4-1.1.6-1.6.3-.6-.2-1-.8-1-1.4v-45C3 .7
										3.7 0 4.5 0h39c.8 0 1.5.7 1.5 1.5v45c0 .6-.4
										1.2-.9 1.4-.2.1-.4.1-.6.1zM24 26c.8
										0 1.6.3 2.2.9l15.8 16V3H6v39.9l15.8-16c.6-.6
										1.4-.9 2.2-.9z">
                                    </path>
                                </svg>
                            </div>
                        </div>
                        <p class="likes">{{ post.avis }} likes</p>
                        <p class="message">
                            <b>{{ post.content }}</b>
                        </p>
                        <a href="{{ path('show_post',{id:post.id}) }}">
                            <h4 class="comments">Voir tous les commentaires</h4>
                        </a>
                        <h5 class="postTime">publié le {{ post.createdAt |date('d/m/y') }} à {{ post.createdAt |date('H:i') }}</h5>
                        <div class="addComments">
                            <div class="reaction">
                                <h3>
                                    <i class="far fa-smile"></i>
                                </h3>
                            </div>

                        </div>
                    </div>
                </div>
                {% endfor %}
            </div>
            <div class="col-3">
                <div class="card">
                    <h4>Suggestions For You</h4>
                    {% for user in users %}
                        {% if(user != userConnected) %}
                    <div class="top">
                        <div class="userDetails">
                            <div class="profilepic">
                                <div class="profile_img">
                                    <div class="image">
                                        {% if user.profil.image is not null  %}
                                            <img src="{{ vich_uploader_asset(user.profil.image, 'imageFile') }}" style="    overflow:hidden;
    -webkit-border-radius:100%;
    -moz-border-radius:100%;
    border-radius:100%;
    width:25px;
    height:25px;" >
                                        {% else %}
                                            <img src="https://media.geeksforgeeks.org/wp-content/uploads/20220609093221/g2-200x200.jpg" alt="img12">
                                        {% endif %}
                                    </div>
                                </div>
                            </div>
                            <h3>{{user.username }}<br>
                                <span>Follows You</span>
                            </h3>
                        </div>
                        <div>
                            <a href="#"
                               class="follow">follow
                            </a>
                        </div>
                    </div>
                        {% endif %}
                    {% endfor %}
                </div>

                <!-- Our Footer Section will start from Here -->
                <div class="footer">
                    <a class="footer-section" href="#">About</a>
                    <a class="footer-section" href="#">Help</a>
                    <a class="footer-section" href="#">API</a>
                    <a class="footer-section" href="#">Jobs</a>
                    <a class="footer-section" href="#">Privacy</a>
                    <a class="footer-section" href="#">Terms</a>
                    <a class="footer-section" href="#">Locations</a>
                    <br>
                    <a class="footer-section" href="#">Top Accounts</a>
                    <a class="footer-section" href="#">Hashtag</a>
                    <a class="footer-section" href="#">Language</a>
                    <br><br>
                    <span class="footer-section">
						© 2023 INSTAGRAM FROM FACEBOOK
					</span>
                </div>
            </div>
        </div>
    </main>
{% endblock %}
